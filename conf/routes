# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# An example controller showing a sample home page



POST    /save/data                    controllers.BankController.saveData(request : Request)
POST    /save/LoginService            controllers.BankController.saveLoginService(request : Request)
POST    /save/SendMoneyService        controllers.BankController.saveSendMoneyService(request : Request)
POST    /save/DeactivateService       controllers.BankController.saveDeactivateService(request : Request)
POST    /save/ActivateService         controllers.BankController.saveActivateService(request : Request)
GET    /save/ProfileDetailsService    controllers.BankController.saveProfileDetailsService(request : Request)
GET    /save/ViewListService          controllers.BankController.ViewListService()
GET    /render/AccountStatement       controllers.BankController.renderAccountStatement(request : Request)

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.versioned(path="/public", file: Asset)


